    <body>

        <nav class="navbar sticky-top navbar-dark bg-dark navbar-expand-lg">
            <div class="container-fluid">
                <div class="navbar-header">
                    <a class="navbar-brand" href="/vehicles"><h3>Radiant Auto</h3></a>
                </div>
                <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="collapse navbar-collapse" id="navbarSupportedContent">
                    <ul class="navbar-nav mr-auto mt-2 mt-lg-0">
                            <li class="nav-item collapse"><a class="nav-link" href="/vehicles">Home</a></li>
                            <li class="nav-item"><a class="nav-link" href="/about">About</a></li>
                            <li class="nav-item">
                                <a class="nav-link" href="https://www.facebook.com/RadiantAutoLLP">Connect with Us on Facebook! <i class="text-primary fab fa-facebook"></i></a> 
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" href="tel:910-236-6077"><i class=" text-success fas fa-phone-alt"></i> Call Now! </a>
                            </li>
                        <% if(!currentUser){ %>
                            <li class="nav-item"><a class="nav-link" href="/login">Dealer Login</a></li>
                            <!-- <li class="nav-item"><a class="nav-link" href="/register">Signup</a></li> -->
                        <% } else { %>
                            <li class="nav-item"><a class="nav-link" href="#">Signed In As <%= currentUser.username %> </a> </li>
                            <li class="nav-item"><a class="nav-link" href="/logout">Logout</a></li>
                        <% } %>
                        
                    </ul>
                </div>
            </div>
        </nav>






        <div class="container p-2">
            <% if(error && error.length > 0) { %>
                <div class="alert alert-danger" role="alert">
                    <h1> <%= error %> </h1>
                </div>

                <% } %>
        
                <% if(success && success.length > 0) { %>
                    <div class="alert alert-success" role="alert">
                        <h1> <%= success %> </h1>
                    </div>
        
                    <% } %>

                    
        </div>
       

        